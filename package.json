{
  "name": "harm-reduction-google-dork-guide",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "md:fix": "markdownlint-cli2 --fix \"**/*.md\"",
    "md:check": "markdownlint-cli2 \"**/*.md\"",
    "sort-tables": "node scripts/sort-tables.js --write",
    "sort-tables:check": "node scripts/sort-tables.js --check",
    "lint": "npm run format:check && npm run md:check && npm run sort-tables:check",
    "lint:fix": "npm run format && npm run md:fix && npm run sort-tables"
  },
  "devDependencies": {
    "fast-glob": "^3.3.2",
    "markdownlint-cli2": "^0.16.0",
    "prettier": "^3.2.5"
  }
}
